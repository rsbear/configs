{
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.openAction.for.directory": ".",
  "explorer.file.tabCD": false,
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "svelte", "typescript", "typescriptreact", "javascript"],
  "explorer.keyMappings": {
    "v": "open:vsplit"
  },
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  }
}
